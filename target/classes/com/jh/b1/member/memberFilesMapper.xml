<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.jh.b1.member.MemberFilesMapper">

<!-- id	: memberfilesInsert -->
<!-- autoincrement null or 0 -->

	<insert id="memberfilesInsert" parameterType="com.jh.b1.member.MemberFilesVO">
		insert into memberFiles(id, fname, oname) values(#{id}, #{fname}, #{oname});
	</insert>

	<select id="memberfilesSelect" resultType="com.jh.b1.member.MemberFilesVO" parameterType="com.jh.b1.member.MemberFilesVO">
		select * from memberFiles where fnum=#{fnum}
	</select>





</mapper>	  

 
